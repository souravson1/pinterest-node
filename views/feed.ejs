<%- include('./partials/header') %>
<div class="w-full min-h-screen bg-zinc-800">
    <div class="cards flex flex-wrap gap-10 px-10 py-10">
        <% posts.reverse().forEach(post => { %>
            <div class="card w-52">
                <div class="w-full h-40 bg-zinc-200 rounded-md overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= post.image %>" alt="">
                </div>
                <h5 class="text-md text-white font-semibold mt-2"><%= post.title %></h5>
                <!-- <h5 class="text-sm">description</h5> -->
            </div>
        <% }) %>
    </div>
</div>
<%- include('./partials/footer') %>